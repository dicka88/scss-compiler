{
	"name": "scss-compiler",
	"version": "1.0.0",
	"description": "Compiler online for scss/sass to css",
	"main": "app.js",
	"scripts": {
		"start": "node app.js",
		"build": "postcss src/assets/tailwind.css -o public/css/tailwind.css",
		"watch": "nodemon app.js"
	},
	"keywords": [
		"scss",
		"compiler",
		"sass",
		"css",
		"express"
	],
