		"body-parser": "^1.19.0",
		"cssnano": "^4.1.10",
		"dotenv": "^8.2.0",
		"ejs": "^3.1.3",
		"express": "^4.17.1",
		"morgan": "^1.10.0",
		"node-sass": "^4.14.1",
		"styleflux": "^1.2.0",
		"tailwindcss": "^1.4.6"
